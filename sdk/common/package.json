{
  "name": "common-arbitrary-token-transfer",
  "version": "0.0.1-alpha1",
  "license": "ISC",
  "type": "module",
  "module": "ts-build/index.js",
  "types": "ts-build/index.d.ts",
  "exports": {
    ".": {
      "types": "./ts-build/index.d.ts",
      "require": "./ts-build/index.cjs",
      "import": "./ts-build/index.js"
    }
  },
  "files": [
    "ts-build"
  ],
  "scripts": {
    "build:ts": "rm -rf ts-build && tsc --noEmit -p ./tsconfig.json && tsup",
    "build": "yarn run build:ts",
    "clean": "rm -rf ts-build",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "bn.js": "^5.2.1"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.5",
    "@types/node": "20.17.5",
    "@wormhole-foundation/sdk-base": "^1.3.0",
    "@wormhole-foundation/sdk-definitions": "^1.3.0",
    "tsup": "^8.3.5",
    "tsx": "4.19.2",
    "typescript": "5.6.3"
  },
  "peerDependencies": {
    "@wormhole-foundation/sdk-base": ">= 1.3.0 < 2",
    "@wormhole-foundation/sdk-definitions": ">= 1.3.0 < 2"
  },
  "tsup": {
    "entry": [
      "index.ts"
    ],
    "tsconfig": "tsconfig.build.json",
    "splitting": false,
    "sourcemap": true,
    "minify": false,
    "clean": true,
    "outDir": "ts-build",
    "format": [
      "esm",
      "cjs"
    ],
    "dts": {
      "resolve": true
    },
    "external": [
      "@wormhole-foundation/sdk-base",
      "@wormhole-foundation/sdk-definitions"
    ]
  }
}
